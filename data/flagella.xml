    <species name = "flgMN_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "meche_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "mocha_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />

    <!-- Class 3 operons (mRNA) -->
    <species name = "flgKL_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliDST_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
