    <species name = "fliFGHIJK_d.FliA.FlhDC" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flgBCDEFGHIJ_d.FliA.FlhDC" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flgAMN_d.FliA.FlhDC" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flhBAE_d.FliA.FlhDC" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliAZY_d.FliA.FlhDC" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />

    <!-- Class 2 operons (mRNA) -->
    <species name = "fliLMNOPQR_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliE_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliFGHIJK_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flgBCDEFGHIJ_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
