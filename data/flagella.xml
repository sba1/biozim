    <species name = "FliQ_p" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "FliR_p" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />

    <species name = "FliA.FlgM_p" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "SecretionComplex" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    
    </species>
    
    <listOfParameters>
    </listOfParameters>



    <species name = "fliAZY_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />


    <listofreactions>
    
     <reaction name = "FliA Translation" reversible = "false">
      <listOfReactants>
       <specieReference specie = "fliAZY_m" stoichiometry = "1"/>
       </listOfReactants>
       <listOfProducts>
        <specieReference specie = "FliA_p" stoichiometry = "1"/>
       </listOfProducts>
       
       <kineticLaw formula = "kfliAZY_m_FliA * fliAZY_m">
        <listOfParameters>
         <parameter name = "kfliAZY_m_FliA" value = "3.8"/>
        </listOfParameters>
       </kineticLaw>
      </reaction>

     <reaction name = "FliA Degradation" reversible = "false">
      <listOfReactants>
       <specieReference specie = "FliA_p" stoichiometry = "1"/>
       </listOfReactants>
       <kineticLaw formula = "kFliA_deg * FliA_p">
        <listOfParameters>
         <parameter name = "kFliA_deg" value = "3.8"/>
        </listOfParameters>
       </kineticLaw>
      </reaction>

     <reaction name = "FliA Degradation" reversible = "false">
      <listOfReactants>
       <specieReference specie = "FlhA_p" stoichiometry = "1"/>
       <specieReference specie = "FlhB_p" stoichiometry = "1"/>
       <specieReference specie = "FliO_p" stoichiometry = "1"/>
       <specieReference specie = "FliQ_p" stoichiometry = "1"/>
       <specieReference specie = "FliR_p" stoichiometry = "1"/>
       </listOfReactants>
       
       <listOfProducts>
        <specieReference specie = "SecretionComplex" stoichiometry = "1"/>
       </listOfProducts>
       
       <kineticLaw formula = "kSecretionComplex*FlhA_p*FlhB_p*FliO_p*FliQ_p*FliR_p">
        <listOfParameters>
         <parameter name = "kSecretionComplex" value = "3.8"/>
        </listOfParameters>
       </kineticLaw>
      </reaction>


    </listofreactions>
   
  </listOfSpecies>
 </model>
</sbml>