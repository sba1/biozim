    <species name = "fliLMNOPQR_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliE_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliFGHIJK_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flgBCDEFGHIJ_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flgAMN_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flhBAE_d.FliA" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
