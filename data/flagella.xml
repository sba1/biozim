    <species name = "flhBAE_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliAZY_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />

    <!-- Class 3 operons (promotor) -->
    <species name = "flgKL_d" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliDST_d" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "fliC_d" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "flgMN_d" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
