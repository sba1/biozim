    <species name = "flgMN_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "meche_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "mocha_m" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />

    <!-- Protein -->    
    <species name = "FlgM_p" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "FliA_p" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "FlhA_p" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
    <species name = "FlhB_p" boundaryCondition = "false" initialAmount = "46" compartment = "uVol" />
